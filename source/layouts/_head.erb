<head>
<meta charset="utf-8">

<!-- Always force latest IE rendering engine or request Chrome Frame -->
<meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">

<!-- Use title if it's in the page YAML frontmatter -->
<title><%= current_page.data.title || "Adaptive Training Foundation" %></title>

<meta name="viewport" content="width=device-width, initial-scale=1">

<!-- jQuery -->
<%= javascript_include_tag "https://code.jquery.com/jquery-2.1.3.min.js" %>

<!-- Font Awesome -->
<%= stylesheet_link_tag "http://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css" %>

<!-- Bootstrap -->
<%# link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css"> %>
<%# <link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap-theme.min.css"> %>
<%# <script type="text/javascript" src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js"></script> %>

<!-- Pingendo -->
<%= stylesheet_link_tag "http://pingendo.github.io/pingendo-bootstrap/themes/default/bootstrap.css" %>

<!-- Middleman -->
<%= javascript_include_tag "all" %>
<%= stylesheet_link_tag "overrides" %>

<!-- Favicon -->
<link rel="icon" type="image/png" href="favicon.ico" />

</head>

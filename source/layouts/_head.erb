<head>
<meta charset="utf-8">

<!-- Always force latest IE rendering engine or request Chrome Frame -->
<meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">

<!-- Use title if it's in the page YAML frontmatter -->
<title><%= current_page.data.title || "Adaptive Training Foundation" %></title>

<meta name="viewport" content="width=device-width, initial-scale=1">

<!-- Pingendo Generated Link Tags -->
<%= javascript_include_tag "http://cdnjs.cloudflare.com/ajax/libs/jquery/2.0.3/jquery.min.js" %>
<%= javascript_include_tag "http://netdna.bootstrapcdn.com/bootstrap/3.3.2/js/bootstrap.min.js" %>

<%= stylesheet_link_tag "http://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.3.0/css/font-awesome.min.css" %>
<%= stylesheet_link_tag "http://pingendo.github.io/pingendo-bootstrap/themes/default/bootstrap.css" %>

<!-- Overrides -->
<%= stylesheet_link_tag "overrides" %>

<!-- Favicon -->
<link rel="icon" type="image/png" href="favicon.ico" />

</head>

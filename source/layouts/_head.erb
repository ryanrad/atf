<head>
<meta charset="utf-8">

<!-- Add Fonts -->
<%= stylesheet_link_tag "http://fonts.googleapis.com/css?family=Roboto:400,300,500,700" %>

<!-- Always force latest IE rendering engine or request Chrome Frame -->
<meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">

<!-- Use title if it's in the page YAML frontmatter -->
<title><%= current_page.data.title || "Adaptive Training Foundation" %></title>

<meta name="viewport" content="width=device-width, initial-scale=1">


<!-- jQuery -->
<%= javascript_include_tag "https://code.jquery.com/jquery-2.1.3.min.js" %>

<!-- Font-Awesome -->
<%= stylesheet_link_tag "https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css" %>

<!-- Bootstrap -->
<%= stylesheet_link_tag "https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css" %>
<%= javascript_include_tag "https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js" %>

<!-- Overrides -->
<%= stylesheet_link_tag "overrides" %>

<!-- Favicon -->
<link rel="icon" type="image/png" href="favicon.ico" />

</head>
